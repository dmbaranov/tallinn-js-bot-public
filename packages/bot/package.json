{
  "name": "bot",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "watch": "nodemon dist/bot/src/index.js",
    "dev": "concurrently \"yarn build\" \"yarn watch\""
  },
  "dependencies": {
    "dotenv": "16.0.3",
    "got": "12.6.0",
    "lodash": "4.17.21",
    "telegraf": "4.12.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.7",
    "@types/node": "20.1.0",
    "concurrently": "8.0.1",
    "nodemon": "2.0.22",
    "tsc-alias": "1.8.6",
    "typescript": "5.0.4"
  }
}
